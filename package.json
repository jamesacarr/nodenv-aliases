{
  "name": "nodenv-aliases",
  "version": "1.1.0",
  "private": true,
  "description": "Create aliases for nodenv Node versions",
  "homepage": "https://github.com/nodenv/nodenv-aliases#readme",
  "license": "MIT",
  "author": "Tim Pope (https://tpo.pe)",
  "contributors": [
    "Jason Karns <jason@karns.name> (http://jason.karns.name)"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/nodenv/nodenv-aliases.git"
  },
  "bugs": {
    "url": "https://github.com/nodenv/nodenv-aliases/issues"
  },
  "directories": {
    "bin": "bin",
    "test": "test"
  },
  "scripts": {
    "pretest": "test/setup_nodenv",
    "test": "bats ${CI:+--tap} test",
    "publish:brew": "brew-publish $npm_package_name v$npm_package_version",
    "postversion": "git push origin master v$npm_package_version && npm run publish:brew"
  },
  "devDependencies": {
    "bats": "^0.4.2",
    "bats-assert": "^1.0.1",
    "brew-publish": "git+https://github.com/jasonkarns/brew-publish.git#nodenv-tap"
  }
}
