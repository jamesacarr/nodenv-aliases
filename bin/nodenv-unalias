#!/usr/bin/env bash
#
# Summary: Remove one or more symlinks in the versions directory
#
# Usage: nodenv unalias <alias> [<alias> ...]

# Provide nodenv completions
if [ --complete = "$1" ]; then
  nodenv-alias --list | awk '{print $1}'
  exit 0
fi

[ $# -gt 0 ] || exec nodenv-help alias

for arg in "$@"; do
  nodenv-alias "$arg" --remove
done
